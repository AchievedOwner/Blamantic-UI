<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="app.css" />
    <link rel="stylesheet" href="BlamanticUI.Doc.styles.css" />
    <link rel="stylesheet" href="_content/BlamanticUI/blamantic-ui.css" />
    <link href="highlight/styles/vs2015.min.css" rel="stylesheet" />
    <script src="highlight/highlight.min.js"></script>
    <HeadOutlet @rendermode="InteractiveAuto" />
</head>

<body>
    <Routes @rendermode="InteractiveWebAssembly" />
    <script src="_framework/blazor.web.js"></script>
    <script src="_content/BlamanticUI/blamantic-ui.js" type="module"></script>
    <script type="text/javascript">
        document.addEventListener('DOMContentLoaded', (event) => {
            document.querySelectorAll('pre code').forEach((el) => {
                hljs.highlightElement(el);
            });
        });
    </script>

</body>

</html>
